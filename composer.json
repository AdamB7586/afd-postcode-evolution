{
    "name": "adamb/afd",
    "description": "A PHP Wrapper to retrieve the information given by the AFD Postcode Evolution software (http://www.afd.co.uk)",
    "keywords": ["AFD", "Postcode Evolution", "AFD Addressing", "wrapper"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Binnersley",
            "email": "abinnersley@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "AFD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "AFD\\Tests\\": "tests"
        }
    }
}
